@{
    ViewData["Title"] = "Crear Equipo";
}
<div class="text-center">
    <p class="fuente titulo">Crear Equipo</p>
    <div class="login-box">

        <form action='@Url.Action("GuardarEquipo", "Home")' method="Post" enctype="multipart/form-data">
            <div class="user-box">
                <input type="text" name="NombreEquipo" required autocomplete="off">
                <label>Nombre</label>
            </div>
            <button type="submit" class="fuente2 btn btn-secondary btn-lg mt-4">Guardar</button>
        </form>
    </div>
</div>